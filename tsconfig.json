{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "jsx": "preserve",
      "lib": ["dom", "es2015", "es2017"],
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "strictNullChecks": true,
      "noImplicitAny": true
      },
    "include": ["./src/**/*", "src/types/gatsby-plugin-react-svg.d.ts", "src/types/index.d.ts", "src/types/global.d.ts" ]
  }